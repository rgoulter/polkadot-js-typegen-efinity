{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "composite": true,
    "declaration": true,
    "paths": {
      "@efinity/api-augment": ["node_modules/@efinity/api-augment/src"]
    }
  },
  "exclude": ["node_modules", "build"]
}
